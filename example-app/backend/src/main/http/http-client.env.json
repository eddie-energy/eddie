{
  "local": {
    "baseUrl": "http://localhost:8080",
    "Security": {
      "Auth": {
        "example-app": {
          "Type": "OAuth2",
          "Grant Type": "Password",
          "Client ID": "example-app-client",
          "Scope": "basic",
          "Username": "{{Username}}",
          "Password": "{{Password}}",
          "Token URL": "https://eddie-demo.projekte.fh-hagenberg.at/iam/realms/EDDIE/protocol/openid-connect/token"
        }
      }
    }
  }
}