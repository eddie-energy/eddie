                    <xs:documentation>VAT number of the company.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="VATnumber" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The register number under which the company is known.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="email" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The register number under which the company is known.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
