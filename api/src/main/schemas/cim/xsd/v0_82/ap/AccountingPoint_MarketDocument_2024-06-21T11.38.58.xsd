            </xs:element>
            <xs:element name="surName" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>surname of the customer

                        Either first- and last name is mandatory or Identification is.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="firstName" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>First name of the customer.
                        Either first- and last name is mandatory or Identification is.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="companyName" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Company name if applicable</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Identification" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The contract partner number
                        Either first- and last name is mandatory or Identification is.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateOfBirth" type="xs:date" minOccurs="0"/>
            <xs:element name="dateOfDeath" type="xs:date" minOccurs="0"/>
            <xs:element name="companyRegisterNumber" type="xs:string" minOccurs="0">
